package uRI;
import java.util.*;

public class P1117 {

	public static void main(String[] args) {
		Scanner num = new Scanner(System.in);
		double[] A = new double[100];
		for(int i=0;i<100;i++){
			A[i] = num.nextDouble();
		}
		for(int i=0; i<100;i++){
			if(A[i]<=10.00) {
			System.out.printf("A[%d] = %.1f\n",i,A[i]);
			       }
		     }
		 }
 }	
