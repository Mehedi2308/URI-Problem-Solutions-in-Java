import java.io.IOException;
import java.util.*;
public class Main {
 
    public static void main(String[] args) throws IOException {
		Scanner num = new Scanner(System.in);
		int X = 1; //declaring variables for input
		while(X!=0) {
			X = num.nextInt();
			for(int i=1; i<=X;i++) {
			if(i==X) {
				System.out.print(i+"\n");
				}else{
			System.out.print(i+" ");
				}
			}
		}
    }
 
}
